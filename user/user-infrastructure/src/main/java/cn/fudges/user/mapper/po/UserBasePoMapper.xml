<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.fudges.user.dao.po.UserBasePoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.fudges.user.entity.po.UserBasePo">
        <id column="id" property="id" />
        <result column="user_name" property="userName" />
        <result column="mobile_phone" property="mobilePhone" />
        <result column="nick_name" property="nickName" />
        <result column="auth_status" property="authStatus" />
        <result column="id_num" property="idNum" />
        <result column="real_name" property="realName" />
        <result column="status" property="status" />
        <result column="create_time" property="createTime" />
        <result column="modify_time" property="modifyTime" />
        <result column="tenant_id" property="tenantId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_name, mobile_phone, nick_name, auth_status, id_num, real_name, status, create_time, modify_time, tenant_id
    </sql>

    <!-- 连表查询结果列 -->
    <sql id="Column_List_With_TableName">
        ${tableName}.id, 
        ${tableName}.user_name, 
        ${tableName}.mobile_phone, 
        ${tableName}.nick_name, 
        ${tableName}.auth_status, 
        ${tableName}.id_num, 
        ${tableName}.real_name, 
        ${tableName}.status, 
        ${tableName}.create_time, 
        ${tableName}.modify_time, 
        ${tableName}.tenant_id
    </sql>
</mapper>
